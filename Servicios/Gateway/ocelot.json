{
  "ReRoutes": [
    {
      "DownstreamPathTemplate": "/Catalogos/{path}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 7002
        }
      ],
      "UpstreamPathTemplate": "/catalogos/{path}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put",
        "Delete"
      ],
      "ReRouteIsCaseSensitive": true
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost"
  }
}